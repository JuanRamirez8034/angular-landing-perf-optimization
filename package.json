{
  "name": "landing-perf-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "http-server ./dist/landing-perf-angular -p 8080",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/landing-perf-angular/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lhci": "npm run build && lhci autorun --config=lighthouserc.js",
    "lhci:ssr": "npm run build:ssr && lhci autorun --config=lighthouserc-ssr.js",
    "lint": "ng lint",
    "images:optimize": "npm run images:compress && npm run images:resize",
    "images:resize": "node scripts/resize.mjs",
    "images:compress": "node scripts/compress.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.3",
    "@angular/common": "17.3.3",
    "@angular/compiler": "17.3.3",
    "@angular/core": "17.3.3",
    "@angular/forms": "17.3.3",
    "@angular/platform-browser": "17.3.3",
    "@angular/platform-browser-dynamic": "17.3.3",
    "@angular/router": "17.3.3",
    "@fortawesome/angular-fontawesome": "0.11.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "chart.js": "3.8.0",
    "ng2-charts": "3.1.2",
    "rxjs": "7.5.0",
    "swiper": "8.3.0",
    "tslib": "2.3.0",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.3",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "17.3.3",
    "@angular/compiler-cli": "17.3.3",
    "@lhci/cli": "0.9.0",
    "@types/jasmine": "4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "http-server": "14.1.1",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^8.0.0",
    "jasmine-core": "4.1.0",
    "karma": "6.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "sharp": "^0.33.3",
    "typescript": "5.4.4",
    "webpack-bundle-analyzer": "4.5.0"
  }
}